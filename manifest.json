{
    "manifest_version": 3,
    "name": "Markdown to Wikitext Converter",
    "version": "1.0.1",
    "description": "Convert Markdown text to Wikitext.",
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "icon16.png",
        "48": "icon48.png",
        "128": "icon128.png"
      }
    },
    "permissions": ["activeTab", "scripting"]
  }